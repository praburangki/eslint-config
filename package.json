{
  "name": "@vinicunca/eslint-config-monorepo",
  "version": "1.5.0",
  "private": true,
  "packageManager": "pnpm@7.23.0",
  "description": "Shareable configurations for ESLint & Stylelint",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicunca/eslint-config.git"
  },

  "scripts": {
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag --commit 'chore: release v'"
  },

  "devDependencies": {
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "eslint-define-config": "^1.13.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
