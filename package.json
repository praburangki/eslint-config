{
  "name": "@vinicunca/eslint-config-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "author": "praburangki<https://github.com/praburangki>",
  "scripts": {
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "postinstall": "pnpm -r run stub",
    "build:lib": "pnpm -C eslint build",
    "prepublish": "pnpm run build:lib",
    "postpublish": "pnpm run gen:meta && pnpm -C docs generate",
    "release": "pnpm -C eslint release",
    "gen:meta": "tsx scripts/gen-meta.ts",
    "registry": "pnpm publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@types/node": "^20.7.1",
    "@vinicunca/eslint-config": "workspace:*",
    "@vinicunca/perkakas": "^0.0.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "eslint-define-config": "^1.23.0",
    "rimraf": "^5.0.5",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "publishConfig": {
    "directory": "eslint"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8.50.0"
    },
    "patchedDependencies": {
      "eslint-define-config@1.23.0": "patches/eslint-define-config@1.23.0.patch"
    }
  }
}
