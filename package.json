{
  "name": "@vinicunca/eslint-config-monorepo",
  "type": "module",
  "version": "2.0.0-alpha.0",
  "private": true,
  "packageManager": "pnpm@8.6.8",
  "author": "praburangki<https://github.com/praburangki>",
  "scripts": {
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepare": "pnpm -r run stub",
    "build:eslint": "pnpm -C eslint build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag --commit 'chore: release v'"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@vinicunca/eslint-config": "workspace:*",
    "@vinicunca/perkakas": "^0.0.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "eslint-define-config": "^1.23.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8.50.0"
    }
  }
}
