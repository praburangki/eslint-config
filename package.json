{
  "name": "@vinicunca/eslint-config-monorepo",
  "type": "module",
  "version": "2.15.1",
  "private": true,
  "packageManager": "pnpm@9.14.4",
  "author": "praburangki<https://github.com/praburangki>",
  "scripts": {
    "lint": "TIMING=1 eslint .",
    "test": "pnpm -r run test",
    "postinstall": "is-ci || pnpm -r run stub",
    "release": "bumpp -r",
    "gen:meta": "tsx scripts/gen-meta.ts",
    "build:docs": "pnpm --filter @vinicunca/eslint-config-docs build",
    "fb:deploy": "firebase deploy --only hosting:eslint",
    "publish:ci": "pnpm --filter @vinicunca/eslint-config publish:ci"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.17.3",
    "@eslint/config-inspector": "^0.5.6",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin-migrate": "^2.11.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@vinicunca/eslint-config": "workspace:*",
    "@vinicunca/perkakas": "^1.0.6",
    "astro-eslint-parser": "^1.1.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-solid": "^0.14.4",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-typegen": "^0.3.2",
    "execa": "^9.5.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "is-ci": "^3.0.1",
    "jiti": "^2.4.1",
    "lint-staged": "^15.2.10",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-slidev": "^1.0.5",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.4.0",
    "svelte-eslint-parser": "^0.43.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue": "^3.5.13"
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.1",
    "@typescript-eslint/utils": "^8.16.0",
    "eslint": "^9.15.0",
    "tsx": "^4.19.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
