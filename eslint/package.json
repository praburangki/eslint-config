{
  "name": "@vinicunca/eslint-config",
  "type": "module",
  "version": "2.7.9",
  "description": "Vinicunca ESLint config",
  "author": {
    "name": "praburangki",
    "email": "praburangki@gmail.com",
    "url": "https://praburangki.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicunca/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/eslint-config/issues"
  },
  "keywords": [
    "JavaScript Linter",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "policy",
    "quality",
    "simple",
    "style",
    "style checker",
    "style linter"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint -v",
    "test": "vitest",
    "build": "pnpm run typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm --clean",
    "typegen": "tsx scripts/typegen.ts",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.10.1",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@unocss/eslint-plugin": "^0.62.2",
    "@vitest/eslint-plugin": "^1.0.3",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-flat-config-utils": "^0.3.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.2.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/fs-extra": "^11.0.4",
    "eslint-typegen": "^0.3.1",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "react": "^18.3.1",
    "tsup": "^8.2.4",
    "vue": "^3.4.38"
  }
}
