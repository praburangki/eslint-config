  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "JavaScript Linter",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "policy",
    "quality",
    "simple",
    "style",
    "style checker",
    "style linter"
  ],
  
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicunca/eslint-config.git"
  },
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp package.json --commit --push --tag --commit 'chore: release v'"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "eslint-plugin-n": "^16.1.0"
