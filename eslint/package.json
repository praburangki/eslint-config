{
  "name": "@vinicunca/eslint-config-alpha",
  "version": "2.0.0-alpha.0",
  "packageManager": "pnpm@8.7.1",
  "author": {
    "name": "praburangki",
    "email": "praburangki@gmail.com",
    "url": "https://praburangki.dev"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/eslint-config/issues"
  },
  "engines": {
    "node": ">=18.0.0"
