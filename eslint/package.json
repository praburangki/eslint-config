    "style linter"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint -v",
    "test": "vitest",
    "build": "pnpm run typegen && tsup --format esm,cjs --clean --dts",
