{
  "name": "@vinicunca/eslint-config",
  "type": "module",
  "version": "2.0.13",
  "description": "Vinicunca ESLint config",
  "author": {
    "name": "praburangki",
    "email": "praburangki@gmail.com",
    "url": "https://praburangki.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicunca/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/eslint-config/issues"
  },
  "keywords": [
    "JavaScript Linter",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "policy",
    "quality",
    "simple",
    "style",
    "style checker",
    "style linter"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint -v",
    "test": "vitest",
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "release": "bumpp package.json --commit --push --tag --commit 'chore: release v'"
  },
  "peerDependencies": {
    "eslint": ">=8.55.0"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@eslint-types/jsdoc": "^46.9.1",
    "@eslint-types/typescript-eslint": "^6.16.0",
    "@eslint-types/unicorn": "^50.0.1",
    "@stylistic/eslint-plugin": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@unocss/eslint-plugin": "^0.58.1",
    "@vinicunca/eslint-plugin-vinicunca": "^1.0.10",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-jsonc": "^2.11.2",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-yml": "^1.11.0",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@eslint-stylistic/metadata": "^1.5.1",
    "@types/eslint": "^8.56.0",
    "@types/fs-extra": "^11.0.4",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "vue": "^3.3.13"
  }
}
